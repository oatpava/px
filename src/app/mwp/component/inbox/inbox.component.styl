.sidenav {
    padding-top: 10px;
    padding-bottom: 100px; 
}
.searchFilter {
    width: 100%;
    margin-top: 10px;
}

.icon {
    color: #9e9e9e;
}
.icon-define {/*ยังไม่ทำ สีเทา เด้ง*/
    color: #f79801;
    position: relative;
    animation-name: alert;
    animation-duration: 1s;
    animation-iteration-count: infinite;
}
@keyframes alert {/*เด้ง*/
    0%   {bottom:0px;}
    100% {bottom:3px;}
}
.icon-flag {/*ทำแล้ว สีเขียว*/
    color: #76b759;
}
.icon-expire {/*ทำแล้ว สีแดง*/
    color: red;
}

.fixed-size {
    table-layout: fixed;
    word-wrap: break-word;
}

.icon-flipped
    transform: scaleX(-1)
    -moz-transform: scaleX(-1)
    -webkit-transform: scaleX(-1)
    -ms-transform: scaleX(-1)   
 .icon-unopen 
    color: #f79801;


   