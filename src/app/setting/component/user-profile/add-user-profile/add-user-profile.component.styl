md-sidenav-layout{
    position: absolute;
    margin-top: 40px;
}
.icon-button{
    cursor: pointer;
    color: #707070;
    padding: 5px;
    border-radius: 4px 4px;
    background-color: #f5f5f5;
}
.icon-button:hover{
    border-radius: 4px 4px;
    background-color: #f5f5f5;
}
.block-icon-Option{
    display: inherit;
}
.displayShow{
    display: inherit;
}
.displayIconNone{
    display: none;
}
.listProfile
    opacity: 0.5
    transition: 0.5s ease
.listProfile:hover
    opacity: 1.0
    background-color: #8fc2f5

:host/deep/.ui-dialog.ui-widget .ui-dialog-titlebar {
    background: #8fc2f5;
    padding-top: 0.5rem;
    padding-left: 0.5rem;
    padding-right: 0.5rem;
    padding-bottom: 0.5rem;
    text-align: left;
}
:host/deep/.ui-dialog.ui-widget .ui-dialog-content {
    padding-top: 0rem;
    padding-left: 0.5rem;
    padding-right: 0.5rem;
    padding-bottom: 0.5rem;
    overflow: auto;
    min-width: 200px;
    min-height: 100px;
    max-width: 600px;
    max-height: 300px;
}